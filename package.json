{
  "name": "glslEditor",
  "version": "0.0.23",
  "description": "Simple GLSL Fragment Shader Editor",
  "main": "src/electron.js",
  "scripts": {
    "dev:browser": "vite --config ./vite/vite.config.browser.ts --open",
    "build": "gulp",
    "dev:electron": "vite --config ./vite/vite.config.electron.ts",
    "start": "electron .",
    "package-local": "electron-packager . --out ./build --overwrite",
    "package-all": "electron-packager . --out ./build --overwrite --all",
    "postinstall": "ln -nsf ../src/js node_modules/app",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint *.js src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patriciogonzalezvivo/glslEditor.git"
  },
  "keywords": [
    "glsl",
    "shader",
    "editor"
  ],
  "author": "Patricio Gonzalez Vivo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patriciogonzalezvivo/glslEditor/issues"
  },
  "homepage": "https://github.com/patriciogonzalezvivo/glslEditor#readme",
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "csswring": "^4.0.0",
    "electron": "^21.2.3",
    "jscs": "^2.4.0",
    "modernizr": "^3.1.0",
    "postcss-color-hex-alpha": "^2.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-import": "^7.1.0",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "codemirror": "5.8.0",
    "cross-storage": "^1.0.0",
    "document-register-element": "^1.9.1",
    "electron-builder": "^23.6.0",
    "eslint": "^4.11.0",
    "glslCanvas": "^0.2.5",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-electron": "^0.10.3",
    "xhr": "^2.2.0"
  },
  "overrides": {
    "graceful-fs": "^4.2.10"
  },
  "browserify-shim": {}
}
